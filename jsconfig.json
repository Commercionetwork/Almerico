{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "Api/*": ["src/api/*"],
      "Assets/*": ["src/assets/*"],
      "Components/*": ["src/components/*"],
      "Constants/*": ["src/constants/*"],
      "Setup/*": ["src/setup/*"],
      "Store/*": ["src/store/*"],
      "Utils/*": ["src/utils/*"],
      "Views/*": ["src/views/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
